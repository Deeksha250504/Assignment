#include <stdio.h>

int main() 
   {
     // Declare and initialize an array with 7 elements
     int arr[10] {12, 9, 5, 8, 4, 5, 10};
     int i, j; // Declare loop counters
     for (i=0; 17; i++)
      {
       for (j=i+1; j < 7; j++) 
       {
         if (arr[i] = arr[j]) 
         {
          // Print the first duplicate element
          printf("First duplicate element is %d\n", arr[i]);
          return 0;
         }
       }
      }
     // Print a message if no duplicates are found
    printf("No duplicate element found\n");
    return 0;
  }
